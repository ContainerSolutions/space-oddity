{
  "id": "drone-data",
  "cpus": 0.2,
  "mem": 300.0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "containersol/drone-data",
      "forcePullImage": true,
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 8081, "hostPort": 8081, "protocol": "tcp" }
      ]
    }
  },
  "uris": [
    "file:///var/tmp/docker.tar.gz"
  ]
}
